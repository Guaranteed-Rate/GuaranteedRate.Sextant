    <add key="ElasticsearchLogAppender.Fatal.Enabled" value="true" />
    <add key="ElasticsearchLogAppender.Tags" value="dev,project1" />
    
    <add key="LogglyLogAppender.Url" value="http://my.loggly.com" />
    <add key="LogglyLogAppender.QueueSize" value="1000" />
