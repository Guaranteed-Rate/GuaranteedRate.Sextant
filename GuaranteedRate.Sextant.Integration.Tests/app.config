    <add key="LogglyLogAppender.Enabled" value="true" />
    <add key="LogglyLogAppender.ApiKey" value="3420bf81-80fb-497b-8168-1e63dfb2f033" />
    <add key="LogglyLogAppender.Url" value="https://logs-01.loggly.com" />
